isbn = {1759-6653},
issn = {1759-6653},
journal = {Genome Biology and Evolution},
keywords = {archaea,archaeal ancestor,ljungdahl pathway,methanogenesis,pathway loss,wood},
number = {6},
pages = {evw114},
pmid = {27189979},
title = {{Methanogenesis and the Wood-Ljungdahl pathway: an ancient, versatile, and fragile association}},
