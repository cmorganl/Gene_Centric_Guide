file = {:mnt/sdb/Scientific{\_}papers/NatureMicrobiology{\_}Borrel{\_}2019.pdf:pdf},
issn = {2058-5276},
journal = {Nature microbiology},
number = {4},
pages = {603--613},
pmid = {30833729},
title = {{Wide diversity of methane and short-chain alkane metabolisms in uncultured archaea.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/30833729 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6453112},
volume = {4},
year = {2019}
