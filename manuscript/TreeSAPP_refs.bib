publisher = {Oxford University Press},
title = {{GraftM: a tool for scalable, phylogenetically informed classification of genes within metagenomes}},
url = {https://academic.oup.com/tropej/article-lookup/doi/10.1093/tropej/fmw080 https://academic.oup.com/nar/advance-article/doi/10.1093/nar/gky174/4942468},
volume = {46},
